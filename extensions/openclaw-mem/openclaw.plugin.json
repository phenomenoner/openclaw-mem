{
  "id": "openclaw-mem",
  "name": "OpenClaw Mem",
  "description": "Capture tool results into JSONL for openclaw-mem ingestion",
  "kind": "memory",
  "version": "0.1.0",
  "configSchema": {
    "type": "object",
    "additionalProperties": false,
    "properties": {
      "enabled": { "type": "boolean", "default": true },
      "outputPath": { "type": "string", "default": "~/.openclaw/memory/openclaw-mem-observations.jsonl" },
      "includeTools": { "type": "array", "items": { "type": "string" } },
      "excludeTools": { "type": "array", "items": { "type": "string" } }
    }
  }
}
